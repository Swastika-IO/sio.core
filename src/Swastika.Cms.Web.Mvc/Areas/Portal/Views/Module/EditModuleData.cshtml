@model BEModuleDataViewModel

@{
    ViewData["Title"] = "Edit Module Data";
    //Layout = "~/Areas/Portal/Views/Shared/_Layout.cshtml";
    var types = Html.GetEnumSelectList<Swastika.Cms.Lib.SWCmsConstants.DataType>();
}
<!-- Main content -->
<section class="content container-fluid">
    <form asp-controller="@ViewContext.RouteData.Values["controller"].ToString()"
          asp-action="@ViewContext.RouteData.Values["action"].ToString()">

        @await Html.PartialAsync("EditableSides/_Editable_ActionsBar")
        <div class="row">
            <section class="col-lg-9">
                @await Html.PartialAsync("_Editable_Data", Model)
            </section>
            <section class="col-lg-3">
                @await Html.PartialAsync("_EditableSide_Data", Model)
            </section>
        </div>
    </form>
</section>
<!-- /.content -->