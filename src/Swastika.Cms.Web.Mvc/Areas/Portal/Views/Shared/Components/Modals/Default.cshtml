                                        <input type="file" class="custom-file-input" name="file" id="file" data-file="mediaFile.file">
                                        <label class="custom-file-label" for="file">Choose file</label>
                                    </div>*@

                                <label class="custom-file">
                                    <img src="{{mediaFile.fullPath}}" name="pic" class="d-block" style="max-height: 150px; width: auto; margin: 0 auto;" accept="image/*" width="100%" />
                                    <input type="hidden" ng-model="mediaFile.folder" class="folder-val" />
                                    <input type="file" name="pic" class="" data-file="mediaFile.file">
                                    <input type="text" class="form-control d-none" ng-model="mediaFile.fullPath">
                                </label>
                            </div>
                            <div class="btn-group mr-2" role="group" aria-label="First group">
                                <button type="button" ng-click="uploadMedia()" class="btn btn-secondary"><i class="mi mi-Upload"></i> Upload</button>
                            </div>
                        </div>
                    </div>
                </div>
                @*<div class="row">
                        <div class="col-lg-10">
                            <div class="col-lg-4">
                                <input type="file" id="file" />
                            </div>
                        </div>
                        <div class="col-lg-2" style="margin-bottom: 10px;">
                            <a href="#" class="btn btn-success btn-block" onclick="SW.Common.loadFileStream('Images')">Submit file...</a>
                        </div>
                    </div>*@
                <table class="table table-sm table-condensed">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">
                                Title
                            </th>
                            <th scope="col">
                                Description
                            </th>
                            <th scope="col">
                                Full Path
                            </th>
                            <th scope="col">
                                Content
                            </th>
                            <th scope="col" width="5%" class="text-right">
                                Actions
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="item in mediaData.items track by $index">
                            <td>
                                {{item.title}}
                            </td>
                            <td>
                                <small>
                                    {{item.description}}
                                </small>
                            </td>
                            <td>

                                <div class="input-group input-group-sm mb-3">
                                    <input id="path-modal-{{$index}}" value="{{item.fullPath}}" onclick="this.select()" class="form-control form-control-sm" readonly />
                                    <div class="input-group-append">
                                        <button class="btn btn-outline-secondary btn-clipboard" type="button" data-clipboard-target="#path-modal-{{$index}}"><i class="mi mi-Copy"></i></button>
                                    </div>
                                </div>
                            </td>
                            <td>
                                <img height="50px" src="{{item.fullPath}}" />
                            </td>
                            <td>
                                <div class="btn-group btn-group-sm" role="group" aria-label="Actions">
                                    <a href="javascript:void(0)" ng-click="saveMedia(item)" class="btn btn-sm btn-light">
                                        <span class="oi oi-plus text-info"></span>
                                    </a>
                                    <a href="javascript:void(0)" ng-click="removeMedia(item.id)" class="btn btn-sm btn-light">
                                        <span class="fas fa-times text-danger"></span>
                                    </a>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <ul class="pagination pull-right">
                    <li class="page-item">
                        <a class="page-link active" href="javascript:void(0)" ng-click="loadMedia(mediaData.pageIndex-1)"
                           ng-hide="mediaData.pageIndex==0">Prev</a>
                    </li>
                    <li class="page-item" ng-class="{active: n-1==mediaData.pageIndex}" ng-repeat="n in range(mediaData.totalPage)"><a class="page-link" href="javascript:void(0)" ng-click="loadMedia(n-1)">{{n}}</a></li>
                    <li class="page-item">
                        <a class="page-link" href="javascript:void(0)" ng-click="loadMedia(mediaData.pageIndex + 1)"
                           ng-hide="mediaData.pageIndex >= (mediaData.totalPage-1)">Next</a>
                    </li>
                </ul>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

